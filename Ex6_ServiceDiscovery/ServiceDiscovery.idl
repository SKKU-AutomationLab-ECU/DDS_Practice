struct ServiceInfo {
    unsigned long long timestamp;
    string service_name;         // 서비스 이름
    string service_type;         // 서비스 타입 (REST, gRPC, WebSocket 등)
    string endpoint;             // 서비스 엔드포인트
    unsigned long port;          // 포트 번호
    string status;              // 서비스 상태 (ACTIVE, BUSY, ERROR)
    sequence<string> capabilities;  // 서비스 기능 목록
    boolean is_healthy;          // 헬스체크 상태
};